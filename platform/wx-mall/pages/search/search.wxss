.cate-item .b
{
  width: 750rpx;
  padding: 0 6.25rpx;
  height: auto;
  overflow: hidden;
  display: flex;
  flex-wrap: wrap;
}

.cate-item .list-filter{
    height: 80rpx;
    width: 100%;
    background: #fff;
    margin-bottom: 6.25rpx;
}

.cate-item .b .item
{
  float: left;
  background: #fff;
  width: 365rpx;
  margin-bottom:16rpx;
  padding-bottom:15rpx;
  overflow: hidden;
  text-align: center;
  border-radius: 8rpx;
  border:2rpx solid  rgba(168, 167, 167, 0.3);
  display: flex;
  flex-direction: column;
  justify-content: space-between;

}

.cate-item .b .item-b{
  margin-left: 6.25rpx;
}

.cate-item .item .img{
  width: 365rpx;
  height: 302rpx;
}

.cate-item .item .name
{
  display: block;
  width: 365.625rpx;
   height: 74rpx;
  font-family: MicrosoftYaHei;
  font-size: 28rpx;
  margin:8rpx 0 8rpx 0;
  text-align: left;
  display: -webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  text-overflow:ellipsis;
  overflow: hidden;
  padding: 0 20rpx;
  color: #353535;
}

