page
{
  width: 100%;
  height: 100%;
  background: #FAFAFA;
  position: relative;
}
.no-cart{
    width: 100%;
    height: auto;
    margin: 0 auto;
}
.no-cart .c{
    width: 100%;
    height: auto;
    margin-top: 200rpx;
}
.no-cart .c image{
    margin: 0 auto;
    display: block;
    text-align: center;
    width: 258rpx;
    height: 258rpx;
}
.no-cart .c text{
    margin: 0 auto;
    display: block;
    width: 258rpx;
    height: 29rpx;
    line-height: 29rpx;
    text-align: center;
    font-size: 29rpx;
    color: #999;
}
.title-box{
    width: 100%;
    padding-top: 330rpx;
    text-align: center;
    font-size:28rpx;
    color:#999;
    background: url(https://platform-wxmall.oss-cn-beijing.aliyuncs.com/upload/20180727/15015039793f4e.png) no-repeat center 205rpx;
    background-size: 100rpx auto;
    margin-bottom: 50rpx;
}
.to-index-btn{
    color:#fff;
    background:#e64340;
    border-radius:6px;
    width:300rpx;
    height: auto;
    line-height: 70rpx;
    text-align: center;
    font-size:28rpx;
    margin: 0 auto;
    display: block;
}
.cartContainer
{
  width: 750rpx;
  box-sizing: border-box;
  padding: 0 14rpx;
}
.cartContainer .cartItem
{
  width: 724rpx;
  height: 226rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
  margin-top:8rpx;
  border-radius: 8rpx;
}
.cartItem .radioIcon
{
  width: 80rpx;
  height: 226rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.radioIcon .image
{
  width:45rpx;
  height: 45rpx;
  border-radius: 50%;
  border: 1px solid #616161;
  

}
.cartItem .imgContainer
{
  width: 204rpx;
  height: 204rpx;
  margin-top:10rpx;
  margin-bottom: 12rpx;
  background: #aaa;
}
.cartItem .cartItemInfo
{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-content: center;
  flex: 1;
  margin-left: 30rpx;

}
.cartItemInfo .goodsInfo
{
  width: 392rpx;
  margin-right: 28rpx;
  margin-top:36rpx;
  display:-webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2;
  overflow: hidden;
  font-family: MicrosoftYaHei;
  font-size:24rpx;
  color:#353535;
}
.cartItemInfo .goodsType
{
 
  margin-top:28rpx;
  display: flex;
 
}
.goodsType .text
{
  font-family: MicrosoftYaHei;
  font-size: 24rpx;
  color:#878787;
  background: #EBEBEB;
  padding: 7rpx 6rpx 8rpx 8rpx; 
  border-radius: 8rpx;
}
.goodsType .space
{
   height: 100%;
   flex:1;
}
.goodsPriceNumContainer
{
  display: flex;
  justify-content: space-between;
  margin-top:12rpx;
  margin-bottom: 18rpx;
}
.goodsPriceNumContainer .price
{
    font-family: MicrosoftYaHei;
    font-size:28rpx;
    color:#FF0000;
}
.counter
{
  width: 175rpx;
  height: 42rpx;
  font-size: 24rpx;
  display: flex;
  margin-right: 10rpx;
  border-radius: 4rpx;
  overflow: hidden;
}
.counter .minus
{
  border: 2rpx solid rgb(197, 197, 197);
  border-right:none;
  width: 45rpx;
  height: 38rpx;
 line-height: 35rpx;
  text-align: center;
}
.counter .add
{
  border: 2rpx solid rgb(197, 197, 197);
  border-left:none;
  width: 45rpx;
  height: 38rpx;
  text-align: center;
  line-height:35rpx;
}
.counter .num
{
  border: 2rpx solid rgb(197, 197, 197);
  width: 54rpx;
  height: 38rpx;
  text-align: center;
  line-height:38rpx;
}
.totalPriceBar
{
  width: 750rpx;
  height: 118rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #fff;
  margin-top:50rpx;
  position: fixed;
  bottom:4rpx;
  left:0rpx;
  right: 0rpx;

}
.selectedAll
{
  display: flex;
  align-items: center;
}
.selectedAll .img
{
  width:44rpx;
  height: 44rpx;
  border-radius: 50%;
  border: 1px solid #616161;
  margin-left: 36rpx;
}
.selectedAll .selectedAllText
{
  margin-left: 10rpx;
  font-family: MicrosoftYaHei;
  font-size: 28rpx;
  color:#888;
}
.priceTotal .priceTotalText
{
  font-family: MicrosoftYaHei;
  font-size: 28rpx;
  color:#353535;
  margin-left: 26rpx;
}
.priceTotal .priceTotalNum
{
  font-family: MicrosoftYaHei;
  font-size: 28rpx;
  color:#FF0000;
}
.btnContainer
{
  display: flex;
  margin-left: 40rpx;
  margin-right: 18rpx;
}
.btnContainer .btn
{
   width:134rpx;
   height: 82rpx;
   border: 1px solid #545454;
   text-align: center;
   line-height: 82rpx;
   border-radius: 50rpx;
   font-family: MicrosoftYaHei;
   font-size: 28rpx;
   color:#555555;
}
.btnPay
{
  margin-left: 20rpx;
}
.gopay
{
  background-image: url("http://hldfiles.oss-cn-qingdao.aliyuncs.com/selectholiday/upload/20181221/16055882552c97.png");
background-size:cover;
background-repeat:no-reapt;
}
.compete
{
  background-image: url("http://hldfiles.oss-cn-qingdao.aliyuncs.com/selectholiday/upload/20181221/16073714774f3.png");
background-size:cover;
background-repeat:no-reapt;
}