page
{
  background: rgba(250, 250, 250, 1);
  width: 100%;
  height: 100%;
}
.font
{
   font-family: MicrosoftYaHei;
   font-weight: 400;
   color:#353535;
}
.container .title{
  font-weight: 600;
  font-size: 28rpx;
  line-height: 1;
  padding-left:18rpx;
  padding-top:25rpx;
  background-color:rgba(255, 255, 255, 1);
}
.ItemContainer .outerItem
{
  display: flex;
  align-items: center;
  justify-content: space-between;
  background: rgba(255, 255, 255, 1); 
  padding:25rpx 19rpx 25rpx 25rpx;
  border-bottom:2rpx solid rgba(231, 231, 231, 1);
  box-shadow:  0rpx 3rpx 9rpx 0rpx rgba(0, 0, 0, 0.1);
  margin-bottom:8rpx;
 
 
}
.outerItem .picture
{
  width: 170rpx;
  height: 120rpx;
  border-radius: 8rpx;
  background: #409eff;
}
.homeType .homeName
{
  font-size: 28rpx;
  line-height: 36rpx;
}
.homeType .homeTypeNum
{
  font-size: 24rpx;
  line-height: 36rpx;
  color:rgba(178, 178, 178, 1);
}
.pricePer .unit
{
  font-size: 24rpx;
  color:rgba(178, 178, 178, 1);
  padding-left:41rpx;
   
}
.pricePer .price
{
  color:rgba(255, 0, 0, 1);
   font-weight: bold;
}
.outerItem .iconPart
{
  display: flex;
  align-items: center;
}
.iconPart .iconText
{
  color:rgba(18, 139, 255, 1);
  font-size: 24rpx;
  line-height: 36rpx;
}
.iconPart .iconImg
{
  width: 29rpx;
  height: 29rpx;
  background: #409eff;
  margin-left:3px;
  
}
.ItemContainer .innerItem
{
  background:rgba(250,250,250,1);
  padding:28rpx 0rpx 20rpx 0rpx;
  margin:0 25rpx;
  display: flex;
  justify-content: space-between;
  border-bottom:2rpx solid rgba(231, 231, 231, 1);
}
.line
{
  margin-top:6rpx;
  box-shadow: 0 -3rpx 6rpx 2rpx rgba(0, 0, 0, 0.1) ;
}
.innerItemContainer .innerItem:last-child
{
   border-bottom:none;
}
.leftPart .roomName
{
  font-size: 28rpx;
  line-height: 36rpx;
}
.leftPart .roomArea
{
  font-size: 24rpx;
  color:rgba(178, 178, 178, 1);
  line-height: 36rpx;
}
.leftPart .roomPrice .desc
{
  font-size: 24rpx;
  color:rgba(178, 178, 178, 1);
  line-height: 36rpx;
  
}
.leftPart .roomPrice .pricce
{
  font-size: 28rpx;
  color:rgba(255, 0, 0, 1);
  line-height: 36rpx;
}
.rightPart
{
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.counter
{
  display: flex;
  align-items: center;
  justify-content: center;
}
.counter  .reduce,.add
{
  width: 29rpx;
  height: 29rpx;
  background: #409eff;
  border-radius: 50%;
}
.counter .number
{
  font-size: 24rpx;
  padding:0 16rpx;
  color:rgba(68, 68, 68, 1);
}
.repertory
{
  font-size: 22rpx;
  line-height: 36rpx;
  color:rgba(178, 178, 178, 1);
  margin-top:16rpx;
}
.commonItemContainer .commonItem
{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 20rpx;
  background:rgba(255,255,255,1);
  padding:21rpx  26rpx  23rpx 28rpx;
  border-bottom:2rpx solid  rgba(231, 231, 231, 1);
}
.commonItemContainer .commonItem:last-child
{ 
  margin-top: 0rpx;
  border-bottom:none ;
}
.commonItem .infoPart .name
{
  font-size: 28rpx;
  line-height: 36rpx;
}
.commonItem .infoPart .range
{
  font-size: 22rpx;
  line-height: 36rpx;
  color:rgba(178, 178, 178, 1);
}
.commonItem .infoPart .money
{
  font-size: 28rpx;
  line-height: 36rpx;
  color:rgba(255, 0, 0, 1);
}
.tips
{
  margin-top:18rpx;
  margin-left:16rpx;
  padding-bottom:140rpx;
}
.tips .extend
{
 font-size: 22rpx;
 line-height: 36rpx;
 color:rgba(178, 178, 178, 1); 
}
.tips .phone
{
  font-size: 22rpx;
  color:rgba(18, 139, 255, 1);
}
.bottomButton 
{
   position:fixed;
   bottom:0px;
   left:0px;
   right:0px;
   height: 98rpx;
   background: #fff;
   display: flex;
   justify-content: space-between;
   align-items: center;
}
.bottomButton  .totalPrice .icon
{
  font-size: 24rpx;
  color:rgba(255, 0, 0, 1);
  font-weight: bold;
  margin-left:16rpx;
}
.bottomButton  .totalPrice .num
{
  font-size: 28rpx;
  color:rgba(255, 0, 0, 1);
  font-weight: bolder;
}
.bottomButton .nextStep
{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.bottomButton .nextStep .text
{
  color:rgba(18, 139, 255, 1);
  font-size: 28rpx;
  margin-right: 20rpx;
}
.bottomButton .nextStep .button
{
  width: 300rpx;
  background:linear-gradient(-90deg,rgba(65,102,255,1),rgba(103,133,255,1));
  color:#fff;
  font-size:28rpx;
  height:98rpx;
  text-align: center;
  line-height: 98rpx;
}