page 
{
  background-color: rgba(255, 255, 255, 1);
}
.font{
  font-family: MicrosoftYaHei;
  font-weight:400;
}
.container
{
  background-color:rgba(255, 255, 255, 1);
}
.container .topBar
{
  display: flex;
  position:fixed;
  top:0;left:0;
  width: 100%;
  height: 88rpx;
  justify-content:space-between;
  border-bottom: 2rpx solid rgba(230, 230, 230, 1);
  background-color:#fff;
}
.container .topBar .leftPart
{
  height: 100%;
  font-size: 28rpx;
  line-height: 88rpx;
  color:rgba(136, 136, 136, 1);
  box-sizing: border-box;
  padding-left:17rpx;
}
.container .topBar .rightPart
{
   display: flex;
   height: 100%;
   align-items:center;
}
.container .topBar .rightPart .default
{
  height: 100%;
  font-size: 28rpx;
  font-weight: 400;
  line-height: 88rpx;
  text-align: center;
  padding:0 18rpx 0 18rpx;
  
}
.container .topBar .rightPart .default.checked
{
  border-bottom:4rpx solid rgba(18, 139, 255, 1);
   color:rgba(18, 139, 255, 1);
}
.container .topBar .rightPart .line 
{
  width: 4rpx;
  height: 30rpx;
  background: rgba(230, 230, 230, 1);
}
.container .topBar .rightPart .nearBranch
{
  flex: 1;
  height: 100%;
  margin-left:20rpx;
  margin-right: 26rpx;
  font-size: 28rpx;
  line-height: 88rpx;
  color:rgba(85, 85, 85, 1);
  
} 
.container .topBar .rightPart .nearBranch.checked
{
  border-bottom: 4rpx solid #409eff;
   color:rgba(18, 139, 255, 1);
}
.container .ListContainer 
{
   margin-top:88rpx;
}
.container .ListContainer .item
{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 128rpx;
}
.container .ListContainer .item  .infoPart
{
  margin-left:22rpx;
}
.container .ListContainer .item  .infoPart .topPart
{
  display: flex;
  margin-top:20rpx;
  align-items: center;
 
}
.container .ListContainer .item  .infoPart .topPart .icon
{
  width: 40rpx;
  height: 36rpx;
  margin-right:20rpx;
  background-image:url("http://hldfiles.oss-cn-qingdao.aliyuncs.com/selectholiday/upload/20190308/11114680d232e.png");
 background-size:100% 100%;
}
.container .ListContainer .item  .infoPart .topPart .title
{
  font-size: 30rpx;
  font-weight: 400;
  line-height: 1;
  color:#353535;
  vertical-align: middle;
}
.container .ListContainer .item .infoPart .bottomPart
{
  margin-top:20rpx;
  font-size: 24rpx;
  color:#353535;
  line-height: 30rpx;
}
.container .ListContainer .item .distance 
{
  margin-top:30rpx;
  margin-right:16rpx;
}
.container .ListContainer .item .distance .num
{
   font-size: 24rpx;
   line-height: 30rpx;
   color:#353535;
}
.container .ListContainer .item .distance .goIcon
{
  display: flex;
  justify-content: flex-end;
}
.container .ListContainer .item .distance .goIcon view
{
  width: 20rpx;
  height: 35rpx;
  background-image: url("https://hldfiles.oss-cn-qingdao.aliyuncs.com/selectholiday/upload/20181220/103321832e9774.png");
  background-size: 100% 100%;
}
.container .nomore
{
  margin-top:150rpx;
  font-size: 28rpx;
  color:#ddd;
  text-align: center;
}